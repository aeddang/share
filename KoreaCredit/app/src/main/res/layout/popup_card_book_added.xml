<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/_body"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_gravity="top|left"
    tools:context="com.credit.korea.KoreaCredit.MainActivity"
    android:background="#e5e5e5">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/_scrollView"
        android:scrollbars="none"
        android:layout_gravity="top|left"
        android:fillViewport="true">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="top|left"
            android:background="#e5e5e5">

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:background="#22b4ed">

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="13dp"

                    android:layout_gravity="center"
                    android:src="@drawable/book_add_title"
                    android:scaleType="fitCenter"
                    android:id="@+id/_imgTitle" />

                <Button
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:id="@+id/_btnBack"
                    android:layout_gravity="center_vertical|right"
                    android:background="@drawable/btn_back"
                    android:layout_marginRight="10dp" />
            </FrameLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="top|left"
                android:layout_gravity="top|left"
                android:background="#ffffff"
                android:padding="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="10dp">


                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="12dp"
                    android:src="@drawable/book_add_text0"
                    android:layout_gravity="top|left"
                    android:longClickable="true"
                    android:scaleType="fitStart"
                     />
                <Spinner
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:id="@+id/_spinnerCardName"
                    android:layout_gravity="top|left"
                    android:layout_marginTop="10dp"
                    android:gravity="top|left"
                    android:background="@drawable/select_bg0" />
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="12dp"
                    android:id="@+id/_titleBenefitGroup"
                    android:src="@drawable/book_add_text4"
                    android:layout_gravity="top|left"
                    android:longClickable="true"
                    android:scaleType="fitStart"
                    android:layout_marginTop="20dp" />
                <Spinner
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:id="@+id/_spinnerBenefitGroup"
                    android:layout_gravity="top|left"
                    android:layout_marginTop="10dp"
                    android:gravity="top|left"
                    android:background="@drawable/select_bg0" />
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="12dp"
                    android:src="@drawable/book_add_text5"
                    android:id="@+id/_titleBenefitKind"
                    android:layout_gravity="top|left"
                    android:longClickable="true"
                    android:scaleType="fitStart"
                    android:layout_marginTop="20dp" />
                <Spinner
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:id="@+id/_spinnerBenefitKind"
                    android:layout_gravity="top|left"
                    android:layout_marginTop="10dp"
                    android:gravity="top|left"
                    android:background="@drawable/select_bg0" />
                <ImageView
                android:layout_width="match_parent"
                android:layout_height="12dp"
                android:src="@drawable/book_add_text6"
                    android:id="@+id/_titleCompanySort"
                android:layout_gravity="top|left"
                android:longClickable="true"
                android:scaleType="fitStart"
                    android:layout_marginTop="20dp" />
                <Spinner
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:id="@+id/_spinnerCompanySort"
                    android:layout_gravity="top|left"
                    android:layout_marginTop="10dp"
                    android:gravity="top|left"
                    android:background="@drawable/select_bg0" />



                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="12dp"

                    android:src="@drawable/book_add_text1"
                    android:layout_gravity="top|left"
                    android:longClickable="true"
                    android:scaleType="fitStart"
                    android:layout_marginTop="20dp" />

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="30dp"
                    android:layout_gravity="top|left"
                    android:layout_marginTop="10dp">

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/_inputCompany"
                        android:layout_weight="1"
                        android:password="false"
                        android:background="@drawable/input_bg0"
                        android:textSize="13dp"
                        android:textColor="#666666"
                        android:imeOptions="actionDone"
                        android:inputType="text"
                        android:padding="5dp" />

                    <Button
                        android:layout_width="50dp"
                        android:layout_height="fill_parent"
                        android:text="검색"
                        android:id="@+id/_btnSearch"
                        android:background="@drawable/btn_bg1_ac"
                        android:textColor="#fff"
                        android:textSize="11dp"
                        android:layout_marginLeft="5dp"
                        android:layout_gravity="left|center_vertical" />
                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="12dp"

                    android:src="@drawable/book_add_text2"
                    android:layout_gravity="top|left"
                    android:longClickable="true"
                    android:scaleType="fitStart"
                    android:layout_marginTop="20dp" />

                <FrameLayout

                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_gravity="top|left"
                    android:layout_marginTop="10dp">

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:id="@+id/_inputPrice"
                        android:layout_weight="1"
                        android:password="false"
                        android:background="@drawable/input_bg0"
                        android:textSize="13dp"
                        android:textColor="#666666"
                        android:inputType="number"
                        android:imeOptions="actionDone"
                        android:layout_gravity="top|left"
                        android:gravity="left|center_vertical"
                        android:padding="5dp" />

                    <ImageView
                        android:layout_width="17dp"
                        android:layout_height="12dp"
                        android:src="@drawable/unit_won"
                        android:layout_gravity="center_vertical|right"
                        android:longClickable="true"
                        android:scaleType="fitStart"
                        android:id="@+id/imageView15"
                        android:layout_marginRight="5dp"
                        android:layout_marginTop="3dp" />

                </FrameLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="12dp"
                    android:src="@drawable/book_add_text3"
                    android:layout_gravity="top|left"
                    android:longClickable="true"
                    android:scaleType="fitStart"
                    android:layout_marginTop="20dp"
                    />

                <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="30dp"
                    android:layout_gravity="top|left"
                    android:layout_marginTop="10dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/_textDate"

                        android:password="false"
                        android:background="@drawable/select_bg_date"
                        android:textSize="13dp"
                        android:textColor="#666666"

                        android:layout_gravity="top|left"
                        android:padding="5dp"
                        android:text="2001" />

                    <Button
                        android:layout_width="30dp"
                        android:layout_height="match_parent"
                        android:id="@+id/_btnDate"
                        android:layout_gravity="top|right"
                        android:background="@drawable/transparent"
                        android:singleLine="false" />
                </FrameLayout>

                <ImageButton
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:text=""
                    android:id="@+id/_btnComplete"
                    android:layout_gravity="top|left"
                    android:layout_marginTop="20dp"
                    android:src="@drawable/btn_complete_txt2"
                    android:padding="13dp"
                    android:background="@drawable/btn_bg0"
                    android:layout_marginBottom="10dp"
                    android:scaleType="fitCenter" />


            </LinearLayout>

        </LinearLayout>
    </ScrollView>


</FrameLayout>

