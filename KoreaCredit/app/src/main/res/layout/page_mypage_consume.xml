<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/_body"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="top|left"
    tools:context="com.credit.korea.KoreaCredit.MainActivity"
    android:background="#e5e5e5">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none"
        android:id="@+id/_scrollView"
        android:layout_gravity="top|left">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="top|left"
            android:background="#e5e5e5">

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:background="#22b4ed">

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="13dp"

                    android:layout_gravity="center"
                    android:src="@drawable/consume_title"
                    android:scaleType="fitCenter" />
            </FrameLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="top|left"
                android:layout_gravity="top|left"
                android:background="#ffffff"
                android:padding="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp">

                <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_gravity="top|left">

                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="12dp"

                        android:scaleType="fitStart"
                        android:src="@drawable/consume_title0"
                        android:layout_gravity="left|center_vertical" />

                    <Button
                        android:layout_width="60dp"
                        android:layout_height="28dp"
                        android:id="@+id/_btnModify"
                        android:background="@drawable/consume_btn0"
                        android:layout_gravity="center_vertical|right" />
                </FrameLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="12dp"

                    android:src="@drawable/consume_text0"
                    android:layout_gravity="top|left"
                    android:longClickable="true"
                    android:scaleType="fitStart"
                    android:layout_marginTop="10dp" />


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:text="New Text"
                    android:id="@+id/_textUsedYear"
                    android:layout_gravity="top|left"
                    android:gravity="left|center_vertical"
                    android:background="#eeeeee"
                    android:textColor="#444444"
                    android:textSize="13dp"
                    android:padding="5dp"
                    android:layout_marginTop="5dp" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="12dp"

                    android:src="@drawable/consume_text1"
                    android:layout_gravity="top|left"
                    android:longClickable="true"
                    android:scaleType="fitStart"
                    android:layout_marginTop="15dp" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:text="N/A"
                    android:id="@+id/_textUsedMonth"
                    android:layout_gravity="top|left"
                    android:gravity="left|center_vertical"
                    android:background="#eeeeee"
                    android:textColor="#444444"
                    android:textSize="13dp"
                    android:padding="5dp"
                    android:layout_marginTop="5dp" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="12dp"

                    android:src="@drawable/consume_text2"
                    android:layout_gravity="top|left"
                    android:longClickable="true"
                    android:scaleType="fitStart"
                    android:layout_marginTop="15dp" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:text="New Text"
                    android:id="@+id/_textIncome"
                    android:layout_gravity="top|left"
                    android:gravity="left|center_vertical"
                    android:background="#eeeeee"
                    android:textColor="#444444"
                    android:textSize="13dp"
                    android:padding="5dp"
                    android:layout_marginTop="5dp" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="12dp"
                    android:src="@drawable/join_text_5"
                    android:layout_gravity="top|left"
                    android:longClickable="true"
                    android:scaleType="fitStart"
                    android:layout_marginTop="15dp"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:text="New Text"
                    android:id="@+id/_textWhere"
                    android:layout_gravity="top|left"
                    android:gravity="left|center_vertical"
                    android:background="#eeeeee"
                    android:textColor="#444444"
                    android:textSize="13dp"
                    android:padding="5dp"
                    android:layout_marginTop="5dp" />

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginTop="15dp"
                    android:layout_gravity="top|left">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:layout_gravity="top|left"
                        android:layout_marginRight="3dp">

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="12dp"

                            android:src="@drawable/consume_text3"
                            android:layout_gravity="top|left"
                            android:longClickable="true"
                            android:scaleType="fitStart" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:text="New Text"
                            android:id="@+id/_textSex"
                            android:layout_gravity="top|left"
                            android:gravity="left|center_vertical"
                            android:background="#eeeeee"
                            android:textColor="#444444"
                            android:textSize="13dp"
                            android:padding="5dp"
                            android:layout_marginTop="5dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:layout_gravity="top|left"
                        android:layout_marginLeft="3dp">

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="12dp"
                            android:src="@drawable/consume_text4"
                            android:layout_gravity="top|left"
                            android:longClickable="true"
                            android:scaleType="fitStart" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:text="New Text"
                            android:id="@+id/_textMerried"
                            android:layout_gravity="top|left"
                            android:gravity="left|center_vertical"
                            android:background="#eeeeee"
                            android:textColor="#444444"
                            android:textSize="13dp"
                            android:padding="5dp"
                            android:layout_marginTop="5dp"
                            android:editable="false" />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginTop="15dp"
                    android:layout_gravity="top|left">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:layout_gravity="top|left"
                        android:layout_marginRight="3dp">

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="12dp"

                            android:src="@drawable/consume_text5"
                            android:layout_gravity="top|left"
                            android:longClickable="true"
                            android:scaleType="fitStart" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:text="New Text"
                            android:id="@+id/_textFamilly"
                            android:layout_gravity="top|left"
                            android:gravity="left|center_vertical"
                            android:background="#eeeeee"
                            android:textColor="#444444"
                            android:textSize="13dp"
                            android:padding="5dp"
                            android:layout_marginTop="5dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:layout_gravity="top|left"
                        android:layout_marginLeft="3dp">

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="12dp"
                            android:src="@drawable/consume_text6"
                            android:layout_gravity="top|left"
                            android:longClickable="true"
                            android:scaleType="fitStart" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:text="New Text"
                            android:id="@+id/_textBirth"
                            android:layout_gravity="top|left"
                            android:gravity="left|center_vertical"
                            android:background="#eeeeee"
                            android:textColor="#444444"
                            android:textSize="13dp"
                            android:padding="5dp"
                            android:layout_marginTop="5dp" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="#f9f9f9"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:padding="20dp"
                android:layout_marginTop="1dp"
                >

                <ImageView
                    android:layout_width="100dp"
                    android:layout_height="12dp"

                    android:scaleType="fitStart"
                    android:src="@drawable/consume_title2"
                    android:layout_gravity="left|center_vertical" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:src="@drawable/consume_text10"
                    android:layout_gravity="top|left"
                    android:longClickable="true"
                    android:scaleType="fitStart"
                    android:layout_marginTop="15dp"
                    android:id="@+id/imageView5" />

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="22dp"
                    android:layout_gravity="top|left"
                    android:layout_marginTop="10dp" >

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1" >

                        <Button
                            android:layout_width="22dp"
                            android:layout_height="22dp"
                            android:id="@+id/_btnAutoOk"
                            android:background="@drawable/sel_check"
                            android:layout_gravity="top|left" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="12dp"
                            android:src="@drawable/myinfo_text3"
                            android:layout_gravity="left|center_vertical"
                            android:longClickable="true"
                            android:scaleType="fitStart"
                            android:layout_marginLeft="27dp" />
                    </FrameLayout>

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1" >

                        <Button
                            android:layout_width="22dp"
                            android:layout_height="22dp"
                            android:id="@+id/_btnAutoNo"
                            android:background="@drawable/sel_check"
                            android:layout_gravity="top|left" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="12dp"
                            android:src="@drawable/myinfo_text4"
                            android:layout_gravity="left|center_vertical"
                            android:longClickable="true"
                            android:scaleType="fitStart"
                            android:layout_marginLeft="27dp" />
                    </FrameLayout>
                </LinearLayout>

                <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_gravity="top|left"
                    android:src="@drawable/consume_title1"
                    android:layout_marginTop="15dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="11dp"
                        android:src="@drawable/consume_text7"
                        android:layout_gravity="left|bottom"
                        android:longClickable="true"
                        android:scaleType="fitStart"
                        android:layout_marginBottom="5dp" />

                    <Button
                        android:layout_width="78dp"
                        android:layout_height="28dp"
                        android:id="@+id/_btnAddBenefit"
                        android:background="@drawable/consume_btn1"
                        android:layout_gravity="center_vertical|right" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:background="#cccccc"
                    android:layout_gravity="top|left"
                    android:layout_marginTop="5dp"></FrameLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|left"

                    android:weightSum="302"
                    android:id="@+id/_benifitStack">


                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
    </ScrollView>


</FrameLayout>






