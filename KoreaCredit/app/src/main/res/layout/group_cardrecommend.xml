<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/_body"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="top|left"
    tools:context="com.credit.korea.KoreaCredit.MainActivity"
    android:orientation="vertical"
    android:gravity="top|left"
    android:background="#e5e5e5">


    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="40dp"
        android:background="#f9f9f9"
        android:layout_gravity="top|left">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="12dp"
            android:scaleType="fitStart"
            android:src="@drawable/card_recommend_text0"
            android:layout_gravity="left|center_vertical"
            android:layout_marginLeft="15dp" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="40dp"
        android:background="#fff"
        android:layout_marginTop="1dp">

        <ImageView
            android:layout_width="150dp"
            android:layout_height="13dp"

            android:layout_gravity="left|center_vertical"
            android:scaleType="fitStart"
            android:src="@drawable/card_search_text2"
            android:layout_marginLeft="15dp" />

        <Button
            android:layout_width="98dp"
            android:layout_height="30dp"
            android:id="@+id/_btnPaturn"
            android:layout_gravity="right|center_vertical"
            android:layout_marginRight="15dp"
            android:background="@drawable/card_search_btn_paturn" />
    </FrameLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/_paturnStack"
        android:background="#f9f9f9"
        android:layout_gravity="top|left"
        android:layout_marginTop="1dp"
        android:baselineAligned="false"></LinearLayout>

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="40dp"
        android:background="#fff"
        android:layout_marginTop="1dp" >

        <ImageView
            android:layout_width="150dp"
            android:layout_height="13dp"
            android:layout_gravity="left|center_vertical"
            android:scaleType="fitStart"
            android:src="@drawable/card_recommend_text1"
            android:layout_marginLeft="15dp" />

    </FrameLayout>

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="#aaa"
        android:layout_gravity="top|left"></FrameLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/_myCardStack"
        android:background="#fff"
        android:layout_gravity="top|left"
        android:baselineAligned="false" />

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="40dp"
        android:background="#fff"
        android:layout_marginTop="1dp" >

        <ImageView
            android:layout_width="150dp"
            android:layout_height="13dp"
            android:layout_gravity="left|center_vertical"
            android:scaleType="fitStart"
            android:src="@drawable/card_recommend_text2"
            android:layout_marginLeft="15dp" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="#aaa"
        android:layout_gravity="top|left" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/_interestCardStack"
        android:background="#fff"
        android:layout_gravity="top|left"
        android:baselineAligned="false" />

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_gravity="top|left"
        android:layout_marginTop="1dp"
        android:background="#fff"
        android:layout_marginBottom="5dp">

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text=""
            android:id="@+id/_btnComplete"
            android:layout_gravity="top|left"
            android:src="@drawable/card_recommend_btn_text"
            android:padding="13dp"
            android:background="@drawable/btn_bg0"
            android:layout_margin="15dp"
            android:scaleType="fitCenter" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="40dp"
        android:background="#f9f9f9"
        android:layout_gravity="top|left"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="1dp" >

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="15dp"
            android:id="@+id/imageView11"
            android:layout_gravity="left|center_vertical"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:scaleType="fitStart"
            android:src="@drawable/text_info_search_card3" />
    </FrameLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_gravity="top|left"
        android:layout_marginTop="1dp"
        android:background="#fff"
        android:padding="15dp">

        <ImageView
            android:layout_width="150dp"
            android:layout_height="13dp"
            android:layout_gravity="left|center_vertical"
            android:scaleType="fitStart"
            android:src="@drawable/card_search_text4"
            android:id="@+id/imageView14" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginTop="10dp"
            android:id="@+id/_box0" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="• "
                android:textSize="12dp"
                android:lineSpacingExtra="2dp"
                android:textColor="#666"
                android:layout_gravity="top|left" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/_text0"
                android:textSize="12dp"
                android:lineSpacingExtra="2dp"
                android:textColor="#666" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginTop="5dp"
            android:id="@+id/_box1" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="• "
                android:textSize="12dp"
                android:lineSpacingExtra="2dp"
                android:textColor="#666"
                android:layout_gravity="top|left" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/_text1"
                android:textSize="12dp"
                android:lineSpacingExtra="2dp"
                android:textColor="#666" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginTop="5dp"
            android:id="@+id/_box2" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="• "
                android:textSize="12dp"
                android:lineSpacingExtra="2dp"
                android:textColor="#666"
                android:layout_gravity="top|left" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/_text2"
                android:textSize="12dp"
                android:lineSpacingExtra="2dp"
                android:textColor="#666" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>

