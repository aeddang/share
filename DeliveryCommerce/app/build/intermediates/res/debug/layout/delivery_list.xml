<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/_body"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="top|left"
    android:background="#ffffff"
    android:clickable="false"
    tools:context=".MainActivity" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <FrameLayout
            android:id="@+id/_imgBox"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_gravity="top|left"
            android:background="@drawable/noimg" >

            <ImageView
                android:id="@+id/_img"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/transparent"
                android:background="#000000" />

        </FrameLayout>

        <LinearLayout
            android:id="@+id/_textBox"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="top|left"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/_title"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:layout_gravity="top|left"
                android:gravity="top|left"
                android:padding="0dp"
                android:text="TITLE"
                android:textColor="#2f2f2f"
                android:textSize="14dp"
                android:textStyle="bold"
                android:lines="1" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="top|left"
                android:layout_marginTop="5dp" >

                <TextView
                    android:id="@+id/_dis"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="top|left"
                    android:maxLines="2"
                    android:padding="0dp"
                    android:text="1.7km"
                    android:textColor="#2f2f2f"
                    android:textSize="12dp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                     android:gravity="top|left"
                    android:layout_marginLeft="5dp"
                    android:src="@drawable/icon_time" />

                <TextView
                    android:id="@+id/_workTime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:gravity="top|left"
                    android:maxLines="2"
                    android:padding="0dp"
                    android:text="1.7km"
                    android:textColor="#2f2f2f"
                    android:textSize="12dp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                     android:gravity="top|left"
                    android:layout_marginLeft="5dp"
                    android:src="@drawable/icon_dc" />

                <TextView
                    android:id="@+id/_deil"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:gravity="top|left"
                    android:maxLines="2"
                    android:padding="0dp"
                    android:text="1.7km"
                    android:textColor="#2f2f2f"
                    android:textSize="12dp" />

            </LinearLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="top|left"
                android:layout_marginTop="5dp" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|right" >

                    <ImageView
                        android:id="@+id/_star0"
                        android:layout_width="12dp"
                        android:layout_height="wrap_content"
                        android:scaleType="fitCenter"
                        android:src="@drawable/sel_star" />

                    <ImageView
                        android:id="@+id/_star1"
                        android:layout_width="12dp"
                        android:layout_height="wrap_content"
                        android:scaleType="fitCenter"
                        android:src="@drawable/sel_star" />

                    <ImageView
                        android:id="@+id/_star2"
                        android:layout_width="12dp"
                        android:layout_height="wrap_content"
                        android:scaleType="fitCenter"
                        android:src="@drawable/sel_star" />

                    <ImageView
                        android:id="@+id/_star3"
                        android:layout_width="12dp"
                        android:layout_height="wrap_content"
                        android:scaleType="fitCenter"
                        android:src="@drawable/sel_star" />

                    <ImageView
                        android:id="@+id/_star4"
                        android:layout_width="12dp"
                        android:layout_height="wrap_content"
                        android:scaleType="fitCenter"
                        android:src="@drawable/sel_star" />
                </LinearLayout>

                <TextView
                    android:id="@+id/_dc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:text="20%"
                    android:textColor="#fc391f"
                    android:textSize="20dp" />

            </FrameLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="20dp"
            android:layout_height="match_parent"
            android:layout_gravity="top|left"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="10dp"
            android:orientation="vertical"
            android:weightSum="2" >

            <ImageView
                android:id="@+id/_callBtn"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/transparent"
                android:scaleType="fitCenter"
                android:src="@drawable/del_icon_1" />

            <ImageView
                android:id="@+id/_likeBtn"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="top|left"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:background="@drawable/transparent"
                android:scaleType="fitCenter"
                android:src="@drawable/sel_fav_btn" />

        </LinearLayout>

    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_gravity="bottom|left"
        android:background="#eeeeee" >

    </FrameLayout>

</FrameLayout><!-- From: file:/Users/imbc-design01/work/DeliveryCommerce/app/src/main/res/layout/delivery_list.xml -->