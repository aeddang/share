<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/_body"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="top|right"
    android:background="#ffffff"
    android:orientation="vertical"
    tools:context=".MainActivity" >

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_gravity="top|left"
        android:background="#ff7e3e" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|center_horizontal"
            android:text="설정"
            android:textColor="#ffffff"
            android:textColorHint="#ffffff"
            android:textSize="15dp"
            android:textStyle="bold" />

        <ImageButton
            android:id="@+id/_backBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left|center_vertical"
            android:background="@drawable/transparent"
            android:paddingLeft="10dp"
            android:paddingRight="20dp"
            android:src="@drawable/top_icon" />
    </FrameLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="top|left" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="top|left"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="top|left"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="10dp" >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center_vertical"
                    android:layout_weight="1"
                    android:text="아이딜 간격"
                    android:textColor="#323232"
                    android:textSize="15dp" />

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="5dp"
                        android:background="#ff7e3e"
                        android:layout_gravity="left|top"
                        android:layout_marginTop="8dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"></FrameLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="top|left" >

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|top"
                            android:layout_marginRight="20dp">

                            <Button
                                android:id="@+id/_idel0"
                                android:layout_width="19dp"
                                android:layout_height="19dp"
                                android:layout_gravity="center_horizontal|top"
                                android:background="@drawable/sel_switch2" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="매번"

                                android:layout_gravity="left|top"
                                android:textColor="#5e5e5e"
                                android:textSize="12dp"
                                android:layout_marginTop="5dp" />
                        </LinearLayout>
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|top"
                            android:layout_marginRight="20dp">

                            <Button
                                android:id="@+id/_idel1"
                                android:layout_width="19dp"
                                android:layout_height="19dp"
                                android:layout_gravity="center_horizontal|top"
                                android:background="@drawable/sel_switch2" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="1시간"

                                android:layout_gravity="left|top"
                                android:textColor="#5e5e5e"
                                android:textSize="12dp"
                                android:layout_marginTop="5dp" />
                        </LinearLayout>
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|top"
                            android:layout_marginRight="20dp">

                            <Button
                                android:id="@+id/_idel2"
                                android:layout_width="19dp"
                                android:layout_height="19dp"
                                android:layout_gravity="center_horizontal|top"
                                android:background="@drawable/sel_switch2" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="2시간"

                                android:layout_gravity="left|top"
                                android:textColor="#5e5e5e"
                                android:textSize="12dp"
                                android:layout_marginTop="5dp" />
                        </LinearLayout>
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|top">

                            <Button
                                android:id="@+id/_idel3"
                                android:layout_width="19dp"
                                android:layout_height="19dp"
                                android:layout_gravity="center_horizontal|top"
                                android:background="@drawable/sel_switch2" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="3시간"

                                android:layout_gravity="left|top"
                                android:textColor="#5e5e5e"
                                android:textSize="12dp"
                                android:layout_marginTop="5dp" />
                        </LinearLayout>
                    </LinearLayout>

                </FrameLayout>

            </LinearLayout>
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#eeeeee" >
            </FrameLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="10dp" >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|left"
                    android:layout_weight="1"
                    android:text="아이딜"
                    android:textColor="#323232"
                    android:textSize="15dp" />

                <Button
                    android:id="@+id/_idelBtn"
                    android:layout_width="82dp"
                    android:layout_height="28dp"
                    android:layout_gravity="top|left"
                    android:background="@drawable/sel_switch"
                    android:textColor="#323232"
                    android:textSize="12dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#eeeeee" >
            </FrameLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="10dp" >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|left"
                    android:layout_weight="1"
                    android:text="푸쉬알림"
                    android:textColor="#323232"
                    android:textSize="15dp" />

                <Button
                    android:id="@+id/_pushBtn"
                    android:layout_width="82dp"
                    android:layout_height="28dp"
                    android:layout_gravity="top|left"
                    android:background="@drawable/sel_switch"
                    android:textColor="#323232"
                    android:textSize="12dp"
                    android:textStyle="bold" />

            </LinearLayout>
            <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#eeeeee" >
        </FrameLayout>
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="이용약관"
                android:id="@+id/_provision0"
                android:layout_gravity="left|top"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/transparent"
                android:textSize="15dp"
                android:textColor="#323232" />
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#eeeeee" >
            </FrameLayout>
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="개인정보 취급방침"
                android:id="@+id/_provision1"
                android:layout_gravity="left|top"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/transparent"
                android:textSize="15dp"
                android:textColor="#323232" />
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#eeeeee" >
            </FrameLayout>
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="위치기반 서비스 이용약관"
                android:id="@+id/_provision2"
                android:layout_gravity="left|top"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="20dp"
                android:background="@drawable/transparent"
                android:textSize="15dp"
                android:textColor="#323232" />
        </LinearLayout>

    </ScrollView>

</LinearLayout><!-- From: file:/Users/imbc-design01/work/DeliveryCommerce/app/src/main/res/layout/page_setting.xml -->