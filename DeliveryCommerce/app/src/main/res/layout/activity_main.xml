<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/_body"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="top|left"
    tools:context="com.deliverycommerce.MainActivity" >

    <FrameLayout
        android:id="@+id/_viewer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="top|left"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp" >

    </FrameLayout>

    <FrameLayout
        android:id="@+id/_naviBottomBox"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_gravity="bottom" >

    </FrameLayout>

    <Button
        android:id="@+id/_viewerDimed"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:background="@drawable/transparent70" />

    <com.deliverycommerce.NaviLeft
        android:id="@+id/_naviLeft"
        android:layout_width="250dp"
        android:layout_height="match_parent"
        android:layout_gravity="top|left"
        android:layout_marginLeft="-251dp" >

    </com.deliverycommerce.NaviLeft>

    <ImageView
        android:id="@+id/_dimed"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="top|left"
        android:clickable="true"
        android:scaleType="fitXY"
        android:src="@drawable/transparent70" />

    <ProgressBar
        android:id="@+id/_loadingBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center" />

    <ImageView
        android:id="@+id/_intro"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ff7e3d"
        android:scaleType="fitXY"
        android:src="@drawable/intro" />

</FrameLayout>
